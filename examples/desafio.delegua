importar "io"

funcao mostrar_vetor(vec: inteiro[]): vazio {
    para var j = 0; j < vec.tamanho; j++ {
        escreva(vec[j])
        se j + 1 < vec.tamanho
            escreva(" ")
    }
    escreva("\\n")
}

var x, y, contador = 3, 99, 0
var buffer: inteiro[] = []

para var i = 1; i < y; i++ {
    se contador == x {
        mostrar_vetor(buffer)
        contador = 0
        buffer = []
    }
    buffer.adicionar(i)
    contador++
}

se buffer.tamanho > 0
    mostrar_vetor(buffer)
